      <html_code><![CDATA[<input type="text" name="major_rev" disabled size="1" value="REV" populate="1"/>
<input type="button" name="major_rev_increment" value="+" style="width: 16px" onclick="createNewRevision();" />
<script type="text/javascript">
function createNewRevision()
{
  if(!top.aras.confirm(top.aras.getResource("plm", "createnewrevision.confirm")))
  { 
    return;
  }
  var dom = top.aras.createXMLDocument();
  dom.loadXML(parent.item.xml);

  var inItem = dom.selectSingleNode("//Item");
  
  var config_id = inItem.selectSingleNode("config_id");
  var id = inItem.selectSingleNode("id");
  var keyed_name = inItem.selectSingleNode("keyed_name");
  var owned_by_id = inItem.selectSingleNode("owned_by_id");
  var state = inItem.selectSingleNode("state");
  var itemtype = inItem.selectSingleNode("itemtype");
  
  var n = inItem.childNodes.length;
  for (var i = 0; i < n; i++) 
  {
    inItem.removeChild(inItem.childNodes[0]);
  }
  
  inItem.appendChild(config_id);
  inItem.appendChild(id);
  inItem.appendChild(keyed_name);
  inItem.appendChild(owned_by_id);
  inItem.appendChild(state);
  inItem.appendChild(itemtype);
  
  inItem.setAttribute("action", "PE_CreateNewRevision");

  var res = top.aras.soapSend("ApplyItem",inItem.xml);
  if (res.getFaultCode() != 0)
  {
    top.aras.AlertError(res);
    return;
  }
  res = res.getResultsBody();
  dom.loadXML(res);

  top.aras.uiReShowItemEx(parent.itemID, dom.documentElement, parent.viewMode);
}
</script>]]></html_code>
